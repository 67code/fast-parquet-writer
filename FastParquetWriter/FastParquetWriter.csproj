<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <!-- Library Metadata -->
    <AssemblyName>FastParquetWriter</AssemblyName>
    <RootNamespace>FastParquetWriter</RootNamespace>
    <Authors>Syed Abdul Saleem</Authors>
    <Description>A fast and efficient Parquet file writer for .NET applications.</Description>
    <PackageTags>Parquet;Data;Serialization;Performance</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/your-username/fast-parquet-writer</RepositoryUrl>
    <RepositoryType>git</RepositoryType>

    <!-- NuGet Publishing Details -->
    <PackageId>FastParquetWriter</PackageId>
    <Version>1.0.0</Version>
    <Company></Company>
    <Copyright>© 2025 Syed</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Parquet.Net" Version="5.1.1" />
  </ItemGroup>

</Project>
